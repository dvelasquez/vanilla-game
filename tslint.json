{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "quotemark": [
      true,
      "single"
    ],
    "indent": [
      true,
      "spaces",
      2
    ],
    "object-literal-sort-keys": false
  },
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "indent": [
      true,
      "spaces",
      2
    ]
  },
  "rulesDirectory": []
}
